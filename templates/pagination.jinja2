<div class="Pagination">
    {% with category=request.GET.get('category'), order=request.GET.get('order'), order_by=request.GET.get('order_by') %}
        <div class="Pagination-ins">
            {% if page_obj.has_previous() %}
                <a class="Pagination-element Pagination-element_prev"
                   href="{{ url('catalog') }}{% if category %}?category={{ category }}{% else %}?{% endif %}{% if order_by %}&order_by={{ order_by }}{% endif %}{% if order %}&order={{ order }}{% endif %}&page={{ page_obj.previous_page_number() }}">
                    <img src="{{ static('img/icons/prevPagination.svg') }}" alt="prevPagination.svg"/>
                </a>
            {% endif %}

            {% for p in paginator.page_range %}
                {% if page_obj.number == p %}
                    <a class="Pagination-element Pagination-element_current" href="#">
                        <span class="Pagination-text">{{ page_obj.number }}</span>
                    </a>
                {% else %}
                    <a class="Pagination-element"
                       href="{{ url('catalog') }}{% if category %}?category={{ category }}{% else %}?{% endif %}{% if order_by %}&order_by={{ order_by }}{% endif %}{% if order %}&order={{ order }}{% endif %}&page={{ p }}">
                        <span class="Pagination-text">{{ p }}</span>
                    </a>
                {% endif %}
            {% endfor %}

            {% if page_obj.has_next() %}
                <a class="Pagination-element Pagination-element_prev"
                   href="{{ url('catalog') }}{% if category %}?category={{ category }}{% else %}?{% endif %}{% if order_by %}&order_by={{ order_by }}{% endif %}{% if order %}&order={{ order }}{% endif %}&page={{ page_obj.next_page_number() }}">
                    <img src="{{ static('img/icons/nextPagination.svg') }}" alt="nextPagination.svg"/>
                </a>
            {% endif %}
        </div>
    {% endwith %}
</div>